{
  "name": "translator",
  "description": "一个小巧好用的翻译插件~",
  "version": "0.0.1",
  "manifest_version": 3,
  "author": "荣顶",
  "background": {
    "service_worker": "./dist/script/background.js"
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "tabs",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  },
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  },
  "options_page": "options.html"
}
